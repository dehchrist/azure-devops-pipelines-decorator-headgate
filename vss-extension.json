{
    "manifestVersion": 1,
    "id": "pipeline-decorator-headgate",
    "version": "0.1.5",
    "name": "Headgate Pipeline Decorator",
    "publisher": "dechrist",
    "description": "An Azure DevOps Pipeline Decorator that filters out a well known list of tasks",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],
    "categories": [
        "Azure Pipelines"
    ],
    "tags": [
        "DevOps",
        "azure",
        "pipeline",
        "decorator"
    ],
    "galleryFlags": [
        "Free",
        "Private"
    ],
    "icons": {
        "default": "icon.png"
    },
    "content": {
        "details": {
            "path": "README.md"
        },
        "license": {
            "path": "LICENSE"
        }
    },
    "files": [
        {
            "path": "pipeline.headgate.yml",
            "addressable": true,
            "contentType": "text/plain"
        }
    ],
    "links": {
        "overview": {
            "uri": "https://github.com/dehchrist/azure-devops-pipelines-decorator-headgate/blob/master/README.md"
        }
    },
    "repository": {
        "type": "git",
        "uri": "https://github.com/dehchrist/azure-devops-pipelines-decorator-headgate"
    },
    "contributions": [
        {
            "id": "decorator",
            "type": "ms.azure-pipelines.pipeline-decorator",
            "targets": [
                "ms.azure-pipelines-agent-job.pre-job-tasks"
            ],
            "properties": {
                "template": "pipeline.headgate.yml"
            }
        }
    ]
}
